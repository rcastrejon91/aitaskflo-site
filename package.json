{
  "name": "aitaskflo-site",
  "version": "1.0.0",
  "description": "AITaskFlo Site with Enhanced Security",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:security": "jest --testPathPattern=security",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "security:scan": "npm audit && snyk test",
