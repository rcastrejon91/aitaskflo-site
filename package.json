    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7",
    "node-cron": "^3.0.3",
    "ws": "^8.14.2",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "dompurify": "^3.0.5",
    "jsdom": "^22.1.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "winston": "^3.11.0",
